#include <iostream>
using namespace std;

int main()
{
    int a,b,c,t;
    cin >> t;
    for (int i = 1; i <= t; i++){
        cin >> a;
        cin >> b;
        cin >> c;
        if (a + b == c || b + c == a || a + c == b){
            cout << "YES" << endl;
        }
        else 
            cout << "NO" << endl;
    }
    return 0;
}
